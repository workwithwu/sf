    echo '<ul>';

    foreach ($custom_addresses as $region => $addresses) {
        if (!empty($addresses)) {
            echo '<strong>' . esc_html($region) . ':</strong><br>';
            foreach ($addresses as $address) {
                if (!empty($address)) {
                    $parts = explode('|', $address);
                    $address_id = $parts[0];
                    $address_text = isset($address_mapping[$address_id]) ? $address_mapping[$address_id] : $address_id;
                    $additional_info = isset($parts[1]) ? $parts[1] : '';
                    echo '<li>' . esc_html($address_text . ' ' . $additional_info) . '</li>';
                }
            }
            echo '<br>'; // 在每個區域後添加空行以區分不同區域
        }
    }

    echo '</ul>'; 
